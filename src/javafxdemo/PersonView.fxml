<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" maxHeight="600.0" maxWidth="800.0" prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="javafxdemo.PersonViewController">
   <children>
      <Button layoutX="737.0" layoutY="559.0" mnemonicParsing="false" onAction="#backButtonPushed" text="Back" />
      <VBox alignment="CENTER" layoutY="14.0" prefHeight="525.0" prefWidth="800.0" spacing="50.0">
         <children>
            <Label text="Detailed Person View">
               <font>
                  <Font name="Verdana" size="25.0" />
               </font>
            </Label>
            <GridPane maxHeight="150.0" maxWidth="500.0" prefHeight="150.0" prefWidth="500.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="First Name">
                     <font>
                        <Font name="Verdana Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="firstNameLabel" text="Label" GridPane.columnIndex="1">
                     <font>
                        <Font name="Verdana" size="15.0" />
                     </font>
                  </Label>
                  <Label text="Last Name" GridPane.rowIndex="1">
                     <font>
                        <Font name="Verdana Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="lastNameLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1">
                     <font>
                        <Font name="Verdana" size="15.0" />
                     </font>
                  </Label>
                  <Label text="Birthday" GridPane.rowIndex="2">
                     <font>
                        <Font name="Verdana Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="birthdayLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2">
                     <font>
                        <Font name="Verdana" size="15.0" />
                     </font>
                  </Label>
                  <Label text="Age" GridPane.rowIndex="3">
                     <font>
                        <Font name="Verdana Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="ageLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3">
                     <font>
                        <Font name="Verdana" size="15.0" />
                     </font>
                  </Label>
               </children>
            </GridPane>
            <ImageView fx:id="photo" fitHeight="250.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </VBox>
   </children>
</AnchorPane>
